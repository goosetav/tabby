{
  "manifest_version": 3,
  "name": "Tabby",
  "description": "Basic Tab Management",
  "version": "1.7.1",
  "action": {
    "default_icon": "images/tabby-16.png",
    "default_title": "Click to open Tabby"
  },
  "side_panel": {
    "default_path": "popup/tabby.html"
  },
  "permissions": [
    "tabs",
    "sidePanel"
  ],
  "background": {
    "service_worker": "scripts/service-worker.js"
  },
  "icons": {
    "16": "images/tabby-16.png",
    "32": "images/tabby-32.png",
    "48": "images/tabby-48.png",
    "128": "images/tabby-128.png"
  }
}